<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="680.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RegisterController">
   <children>
      <Label alignment="CENTER" layoutX="282.0" layoutY="51.0" prefHeight="44.0" prefWidth="330.0" text="Registration Page">
         <font>
            <Font size="42.0" />
         </font>
      </Label>
      <TextField fx:id="name" layoutX="399.0" layoutY="149.0" prefHeight="45.0" prefWidth="217.0" promptText="Enter your name">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label layoutX="286.0" layoutY="154.0" prefHeight="36.0" prefWidth="105.0" text="Name:">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="397.0" layoutY="219.0" prefHeight="45.0" prefWidth="221.0" promptText="Enter your username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label layoutX="286.0" layoutY="224.0" prefHeight="36.0" prefWidth="105.0" text="Username:">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="password" layoutX="398.0" layoutY="283.0" prefHeight="45.0" prefWidth="222.0" promptText="Enter your password">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label layoutX="287.0" layoutY="288.0" prefHeight="36.0" prefWidth="105.0" text="Password:">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="passwordConfirm" layoutX="397.0" layoutY="353.0" prefHeight="45.0" prefWidth="224.0" promptText="Enter your password again">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label layoutX="209.0" layoutY="358.0" prefHeight="36.0" prefWidth="185.0" text="Confirm Password:">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Button fx:id="submitB" layoutX="412.0" layoutY="432.0" mnemonicParsing="false" onAction="#register" prefHeight="48.0" prefWidth="95.0" text="Submit">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="backB" layoutX="1.0" layoutY="1.0" mnemonicParsing="false" onAction="#goBack" prefHeight="48.0" prefWidth="95.0" text="Go Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="errors" alignment="CENTER" layoutX="132.0" layoutY="491.0" prefHeight="154.0" prefWidth="711.0" wrapText="true">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
